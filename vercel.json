{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/v1"
    },
    {
      "src": "/(.*)",
      "dest": "/v2"
    },
    {
      "src": "/(.*)",
      "dest": "/v3"
    }
  ]
}
