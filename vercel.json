{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/v1/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/v2/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/v3/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }